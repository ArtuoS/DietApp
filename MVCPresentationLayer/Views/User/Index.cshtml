                            method: "GET",
                            url: "/Diet/GenareteDiet/",
                            contentType: "application/json",
                            dataType: "json",
                            traditional: true,
                            data: {
                                'date': JSON.stringify(date),
                                'id': JSON.stringify(id)
                            },
